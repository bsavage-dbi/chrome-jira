{
   "background": {
      "page": "background.html"
   },
   "content_scripts": [{
      "js": [ "parser.js" ],
      "css" : ["tooltip.css"],
      "matches": [ "https://github.cerner.com/*"]
   }],
   "description": "Displays JIRA status heads up on Github.",
   "manifest_version": 2,
   "name": "Cerner Github JIRA Status",
   "options_page": "options.html",
   "permissions": [ "https://github.cerner.com/*", "https://jira.cerner.com/*", "https://jira2.cerner.com/*", "https://jira3.cerner.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.0"
}
