{
   "background": {
      "page": "background.html"
   },
   "content_scripts": [{
      "js": [ "parser.js" ],
      "css" : ["tooltip.css"],
      "matches": [ "https://github.cerner.com/*"]
   }],
   "description": "Turns JIRA key names (such as 'CONNECT-123') info links in webpages for easy navigation.",
   "manifest_version": 2,
   "name": "Cerner Linkify Github to JIRA",
   "options_page": "options.html",
   "permissions": [ "https://github.cerner.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.0"
}
