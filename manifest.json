{
   "background": {
      "page": "background.html"
   },
   "content_scripts": [ {
      "exclude_globs": [ "http://*.acidtests.org/*" ],
      "js": [ "convert_keys.js" ],
      "matches": [ "https://github.cerner.com/*", "https://jira.cerner.com/*", "https://jira2.cerner.com/*", "https://jira3.cerner.com/*"]
   } ],
   "description": "Turns JIRA key names (such as 'CONNECT-123') info links in webpages for easy navigation.",
   "manifest_version": 2,
   "name": "Cerner Linkify Github to JIRA",
   "options_page": "options.html",
   "permissions": [ "https://github.cerner.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.1"
}
